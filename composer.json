{
  "name": "marceltk/util-module",
  "type": "library",
  "description": "Módulo com classes utilitárias ao sistema",
  "keywords": [
    "Util"
  ],
  "license": "MIT",
  "homepage": "https://www.unisoft.net.br/",
  "authors": [
    {
      "name": "Marcel Silva",
      "email": "phpmarcel@gmail.com",
      "role": "Maintainer"
    }
  ],
  "require": {
    "php": "^5.3 || ^7.0",
    "zendframework/zend-filter": "^2.7.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  },
  "extra": {
    "zf": {
      "module": "UtilModule"
    }
  },
  "autoload": {
    "psr-4": {
      "UtilModule\\": [
        "src/",
        "test/"
      ]
    }
  },
  "config": {
    "bin-dir": "bin"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
